    "Definition": [
        "Given two words, predict the preposition that connects the two words in a way that explicitly conveys the meaning of both words. The possible prepositions are: \"of\", \"on\", \"in\", \"for\", \"from\", \"about\", and \"with\"."
    ],
    "Input_language": [
        "English"
    ],
    "Output_language": [
        "English"
    ],
    "Instruction_language": [
        "English"
    ],
    "Domains": [
        "Linguistics"
    ],
    "Positive Examples": [
        {
            "input": "Word1: story \n Word2: pearl",
            "output": "of",
            "explanation": "The preposition correctly connects two words turning it into the sentence 'story of pearl'."
        },
        {
            "input": "Word1: need \n Word2: speech",
            "output": "for",
            "explanation": "The preposition correctly connects two words turning it into the sentence 'need for speech'."
        },
        {
            "input": "Word1: thorn \n Word2: end",
            "output": "on",
            "explanation": "The preposition correctly connects two words turning it into the sentence 'thorn on end'."
        }
    ],
    "Negative Examples": [
        {
            "input": "Word1: city \n Word2: stone",
            "output": "in",
            "explanation": "The preposition doesn't connect the sentence correctly. The correct preposition would be 'city on stone'."
        },
        {
            "input": "Word1: part \n Word2: world",
            "output": "part of world",
            "explanation": "It correctly connected the sentence. But the output is wrong. It supposed to only generate a preposition."
        },
        {
            "input": "Word1: servant \n Word2: gate",
            "output": "castle",
            "explanation": "It suppose to return the preposition that connects two words."
        }
    ],
